page {
  background: #F2EDDF;
}
.personal {
  padding-top: 20rpx;
}
.personal .portraits {
  width: 750rpx;
  height: 152rpx;
  background: #FFF9ED;
  box-shadow: 0px 3rpx 7rpx 0px rgba(83, 60, 26, 0.07);
  margin: 20rpx auto;
  line-height: 152rpx;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-pack: justify;
  -webkit-justify-content: space-between;
          justify-content: space-between;
  padding: 0 40rpx;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
}
.personal .portraits .portraits_text {
  font-size: 36rpx;
  font-weight: 700;
  color: #090909;
}
.personal .portraits .portraits_right {
  height: 92rpx;
  line-height: 92rpx;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-pack: justify;
  -webkit-justify-content: space-between;
          justify-content: space-between;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
}
.personal .portraits .portraits_img {
  width: 93rpx;
  height: 92rpx;
  background: #B3B3B3;
  border-radius: 50%;
  display: inline-block;
  margin-right: 30rpx;
}
.personal .portraits .portraits_img image {
  width: 100%;
  height: 100%;
  border-radius: 50%;
}
.personal .portraits .portraits_name {
  font-size: 36rpx;
  font-weight: 700;
  color: #999999;
  margin-right: 30rpx;
  letter-spacing: 4rpx;
}
.personal .pop_up {
  width: 750rpx;
  height: 318rpx;
  background: #F2EDDF;
  border-radius: 6rpx;
}
.personal .pop_up .pop_title {
  width: 750rpx;
  height: 120rpx;
  margin: 0 auto;
  color: #090909;
  font-size: 32rpx;
  text-align: center;
  line-height: 120rpx;
  border: 1px solid #FFFAF0;
}
.personal .pop_up view {
  color: #090909;
  width: 702rpx;
  height: 60rpx;
  margin: auto;
  line-height: 60rpx;
  text-align: center;
}

