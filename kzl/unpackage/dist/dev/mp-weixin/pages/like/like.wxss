page {
  background: #F2EDDF;
}
page .delete {
  margin-top: 20rpx;
  width: 117rpx;
  height: 356rpx;
  background: #C7744E;
  border-radius: 20rpx 0rpx 0rpx 20rpx;
  font-size: 24rpx;
  font-weight: 700;
  color: #ffffff;
  -webkit-writing-mode: vertical-lr;
          writing-mode: vertical-lr;
  text-align: center;
  padding: 0 40rpx;
}
page .footprint {
  width: 702rpx;
  margin: 0 auto;
  font-size: 28rpx;
}
page .footprint .radio {
  margin: 0 20rpx;
  margin-top: 20%;
  -webkit-transform: scale(0.7);
          transform: scale(0.7);
  color: #C7744E !important;
}
page .footprint .track {
  width: 702rpx;
  height: 357rpx;
  background: #F7EED6;
  box-shadow: 0px 0px 6px 0px rgba(83, 60, 26, 0.3);
  margin: 20rpx 0;
}
page .footprint .track image {
  width: 702rpx;
  height: 294rpx;
}
page .footprint .track .track_title {
  font-size: 30rpx;
  font-weight: 700;
  color: #572808;
  padding: 0 20rpx;
}
page .footprint .manage {
  width: 196rpx;
  height: 60rpx;
  background: #DCC18B;
  border-radius: 28rpx;
  color: #FFFFFF;
  letter-spacing: 12rpx;
  font-size: 28rpx;
  font-weight: 700;
  margin: 60rpx auto;
  line-height: 60rpx;
  text-align: center;
  position: fixed;
  bottom: 40rpx;
  left: 50%;
  margin-left: -98rpx;
}
page .footprint .targe radio {
  margin: 20% 180rpx 0 20rpx;
  -webkit-transform: scale(0.7);
          transform: scale(0.7);
  color: #C7744E !important;
}
page .footprint .targe view {
  width: 148rpx;
  height: 60rpx;
  background: #C7744E;
  border-radius: 28rpx;
  display: inline-block;
  text-align: center;
  line-height: 60rpx;
  color: #FFFFFF;
  margin: 0 20rpx;
}

