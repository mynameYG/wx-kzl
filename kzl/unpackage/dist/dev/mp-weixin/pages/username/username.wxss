page {
  background: #F2EDDF;
}
page .username {
  width: 702rpx;
  margin: auto;
}
page .username .username_box {
  height: 107rpx;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-justify-content: space-around;
          justify-content: space-around;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
  margin-top: 50rpx;
}
page .username .username_box .username_inp {
  width: 702rpx;
  height: 107rpx;
  background: #FFF9ED;
  border-radius: 20px;
  font-size: 36rpx;
  font-weight: 700;
  color: #090909;
  padding: 0 30rpx;
}
page .username .username_box .close {
  position: absolute;
  right: 50rpx;
}
page .username .caution {
  font-size: 24rpx;
  font-weight: 700;
  color: #999999;
  margin: 10rpx 30rpx;
  letter-spacing: 2rpx;
}
page .username .determine {
  width: 290rpx;
  height: 74rpx;
  background: #DCC18B;
  border-radius: 37rpx;
  color: #ffffff;
  margin: auto;
  text-align: center;
  line-height: 74rpx;
  font-size: 36rpx;
  font-weight: 700;
  letter-spacing: 8rpx;
  margin-top: 100rpx;
}

